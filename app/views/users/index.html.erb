<div class="container">
  <div class="row">
    <div class="wrapper col-md-5 col-md-offset-3 col-sm-10">
      <h4>ユーザー一覧</h4>
      <% @users.each do |user| %>
        <div class="topic_profile_image" >
          <%= profile_img(user) %>
        </div>
        <%= user.name %>
        <%= render 'follow_form', user: user %>
      <% end %>
    </div>
  </div>
</div>
